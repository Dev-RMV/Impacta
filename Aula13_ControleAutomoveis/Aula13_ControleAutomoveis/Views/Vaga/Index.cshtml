@model IEnumerable<Aula13_ControleAutomoveis.Models.Vaga>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Numero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorPorHora)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Numero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorPorHora)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VagaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VagaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VagaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
